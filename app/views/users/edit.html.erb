<%= form_with model: @user do |f| %>

<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.select :instrument, User.instruments.keys %>
<%= f.submit "保存"%>
<% end %>